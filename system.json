{
  "type": "v1/system",

  "node_pools": {
    "foo": {
      "instance_type": "t2.large",
      "num_instances": 2
    }
  },

  "components": {
    "aggregator": {
      "type": "v1/reference",

      "git_repository": {
        "file": "lattice.json",
        "tag": "1.x",
        "url": "https://github.com/mlab-lattice/testing__version-aggregator-service.git"
      },

      "parameters": {
        "num_instances": 1
      },
    },

    "service": {
      "type": "v1/reference",

      "git_repository": {
        "file": "lattice.yaml",
        "tag": "1.0.x",
        "url": "https://github.com/mlab-lattice/testing__version-service.git"
      },
    },

    "ubuntu": {
      "type": "v1/job",
      "build": {
        "node_pool": "/:foo",
        "registry": "registry.hub.docker.com",
        "repository": "library/ubuntu",
        "tag": "latest",
        "type": "docker_image"
      }
    }
  }
}
